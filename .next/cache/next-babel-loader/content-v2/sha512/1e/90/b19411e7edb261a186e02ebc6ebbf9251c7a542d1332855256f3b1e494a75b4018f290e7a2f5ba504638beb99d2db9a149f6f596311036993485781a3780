{"ast":null,"code":"var __jsx = React.createElement;\nimport * as React from \"react\";\nimport Card from \"../../components/Card/Card\";\nimport useLocalStorage from \"../../utils/useLocalStorage\";\nimport styled from \"@emotion/styled\";\nimport Button from \"../../components/Button/Button\";\nimport { toast } from \"react-toastify\";\nconst NameBox = styled.div`\n  display: flex;\n  div {\n    margin: 0 16px 0 0;\n  }\n`;\nconst ImageBox = styled.div`\n  margin-right: 8px;\n`;\n\nconst Profile = () => {\n  const [ownedPokemon, setOwnedPokemon] = useLocalStorage(\"ownedPokemon\", \"\");\n\n  const handleRelease = nickName => {\n    const filteredOwnedPokemon = ownedPokemon && ownedPokemon.filter(pokemon => {\n      return pokemon.nickName !== nickName;\n    });\n    toast.success(`${nickName} sudah dilepaskan`, {\n      position: \"top-center\",\n      autoClose: 2000,\n      hideProgressBar: false,\n      closeOnClick: true,\n      pauseOnHover: true,\n      draggable: true,\n      progress: undefined\n    });\n    setOwnedPokemon(filteredOwnedPokemon);\n  };\n\n  const renderNoPokemonOwned = () => {\n    return __jsx(\"div\", null, __jsx(\"p\", null, \" Oh la da la, Kamu tidak punya pokemon\"), __jsx(\"p\", null, \"Go Catch Em\"));\n  };\n\n  return __jsx(React.Fragment, null, ownedPokemon && ownedPokemon.length > 0 && ownedPokemon.map((pokemon, index) => {\n    return __jsx(Card, {\n      key: pokemon.name + index,\n      hoverable: false\n    }, __jsx(ImageBox, null, __jsx(\"img\", {\n      src: pokemon.image,\n      alt: \"\"\n    })), __jsx(\"div\", null, __jsx(NameBox, null, __jsx(\"div\", null, __jsx(\"h3\", null, \"Name\"), __jsx(\"p\", null, pokemon.name)), __jsx(\"div\", null, __jsx(\"h3\", null, \"Nickname\"), __jsx(\"p\", null, pokemon.nickName))), __jsx(\"div\", null, __jsx(Button, {\n      isDanger: true,\n      onClick: () => handleRelease(pokemon.nickName)\n    }, \"release\"))));\n  }), (!ownedPokemon || ownedPokemon.length === 0) && renderNoPokemonOwned());\n};\n\nexport default Profile;","map":null,"metadata":{},"sourceType":"module"}