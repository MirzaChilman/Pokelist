  "scripts": {
    "build": "yarn run clean-dist && cross-env NODE_ENV=production webpack --config configs/webpack.prod.js --color --progress",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=configs/webpack.dev.js",
    "start-prod": "yarn run build && cross-env NODE_ENV=production node server",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
